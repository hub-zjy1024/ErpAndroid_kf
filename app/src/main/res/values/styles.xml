<resources  xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <!--<item name="buttonStyle">@style/zjy_button_light</item>-->
        <item name="android:windowAnimationStyle">@style/AnimFade</item>
    </style>
    <style name="ItemViewPadding">
        <item name="android:paddingLeft">@dimen/item_view_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/item_view_padding_horizontal</item>
        <item name="android:paddingTop">@dimen/item_view_padding_vetical</item>
        <item name="android:paddingBottom">@dimen/item_view_padding_vetical</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AnimFade" parent="@android:style/Animation.Activity"> 
         <item name="android:activityOpenEnterAnimation">@anim/anim_activity_slide_in</item>
          <item name="android:activityCloseExitAnimation">@anim/anim_activity_slide_out</item>
    </style>
