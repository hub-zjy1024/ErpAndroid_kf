<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="wrap_content"
              android:layout_height="match_parent"
              android:background="@color/card_background"
              android:orientation="vertical"
              android:paddingLeft="5dp">

    <TextView
        android:id="@+id/panku_dialog_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="这是明细id"/>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="实盘型号"/>

        <EditText
            android:id="@+id/panku_dialog_partno"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:maxLines="1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="实盘数量"/>

        <EditText
            android:id="@+id/panku_dialog_counts"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:maxLines="1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="实盘厂家"/>

        <EditText
            android:id="@+id/panku_dialog_factory"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:maxLines="1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="实盘描述"/>

        <EditText
            android:id="@+id/panku_dialog_description"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:maxLines="1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="实盘封装"/>

        <EditText
            android:id="@+id/panku_dialog_fengzhuang"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:maxLines="1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="实盘批号"/>

        <EditText
            android:id="@+id/panku_dialog_pihao"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:maxLines="1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="实盘位置"/>

        <EditText
            android:id="@+id/panku_dialog_place"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:maxLines="1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:text="最小包装"/>

        <EditText
            android:id="@+id/panku_dialog_minbz"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:maxLines="1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="盘库备注"/>

        <EditText
            android:id="@+id/panku_dialog_mark"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:maxLines="1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/panku_dialog_panku"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="盘库"/>

        <Button
            android:id="@+id/panku_dialog_reset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"

            android:maxLines="1"
            android:text="解锁"/>

        <Button
            android:id="@+id/panku_dialog_cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_weight="1"
            android:maxLines="1"
            android:text="取消"/>

    </LinearLayout>

</LinearLayout>