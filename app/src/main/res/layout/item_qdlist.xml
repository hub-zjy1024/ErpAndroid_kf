<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

              android:layout_width="match_parent"
              android:orientation="vertical"
              android:layout_height="wrap_content">
    <!--  /*{"制单月份":"201810","供应商":"北京恒成伟业电子有限公司","供应商ID":"9204","开票公司":"北京北方科讯电子技术有限公司
      ","单数":"2","批注":""}*/-->
    <LinearLayout
        android:layout_width="wrap_content"
        android:orientation="horizontal"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/item_qdlist_tv_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:layout_marginLeft="20dp"
            android:id="@+id/item_qdlist_tv_providerid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>


    <TextView
        android:id="@+id/item_qdlist_tv_providername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/item_qdlist_tv_kaipiaoname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/item_qdlist_tv_counts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/item_qdlist_tv_notes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</LinearLayout>